{
  "rabbit_version": "3.12.0",
  "users": [
    {
      "name": "guest",
      "password_hash": "guest",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "exchanges": [
    {
      "name": "delayed.publication",
      "vhost": "/",
      "type": "x-delayed-message",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {
        "x-delayed-type": "direct"
      }
    }
  ],
  "queues": [
    {
      "name": "publication_queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "delayed.publication",
      "vhost": "/",
      "destination": "publication_queue",
      "destination_type": "queue",
      "routing_key": "publication_events",
      "arguments": {}
    }
  ]
} 